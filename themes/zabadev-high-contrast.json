{
  "$schema": "vscode://schemas/color-theme",
  "name": "ZabaDev High Contrast â€“ Windsurf Edition",
  "type": "dark",
  "colors": {
    "editor.background": "#000000",
    "editor.foreground": "#E6E6E6",
    "editor.lineHighlightBackground": "#111111",
    "editor.selectionBackground": "#54007A66",
    "editorCursor.foreground": "#00FF9F",
    "editorWhitespace.foreground": "#222222",
    "activityBar.background": "#090909",
    "sideBar.background": "#0A0A0A",
    "statusBar.background": "#111111",
    "titleBar.activeBackground": "#0D0D0D",
    "button.background": "#FF006E",
    "button.hoverBackground": "#FF3380",
    "terminal.background": "#000000",
    "terminal.foreground": "#FFFFFF",
    "terminal.ansiRed": "#FF004C",
    "terminal.ansiGreen": "#00FF6A",
    "terminal.ansiBlue": "#7A00FF",
    "terminal.ansiYellow": "#F5FF00",
    "terminal.ansiMagenta": "#FF00A6",
    "terminal.ansiCyan": "#00FFF0",
    "terminal.ansiWhite": "#EDEDED",
    "terminal.ansiBlack": "#050505"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#777777",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage.type"],
      "settings": {
        "foreground": "#FF006E",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["variable", "entity.name.variable"],
      "settings": {
        "foreground": "#00FF9F"
      }
    },
    {
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#BA00FF"
      }
    },
    {
      "scope": ["number", "constant.numeric"],
      "settings": {
        "foreground": "#FFB300"
      }
    },
    {
      "scope": ["function", "entity.name.function"],
      "settings": {
        "foreground": "#00FFD1",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["class", "entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#FF0055"
      }
    },
    {
      "scope": ["constant", "entity.name.constant"],
      "settings": {
        "foreground": "#00FF6A"
      }
    },
    {
      "scope": ["invalid", "invalid.deprecated"],
      "settings": {
        "foreground": "#FF0000",
        "fontStyle": "bold underline"
      }
    }
  ]
}
